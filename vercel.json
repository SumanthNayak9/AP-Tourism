{
    "version": 2,
    "functions": {
        "api/chat.js": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/chat",
            "destination": "/api/chat.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/frontend/dist/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/index.html"
        }
    ]
}